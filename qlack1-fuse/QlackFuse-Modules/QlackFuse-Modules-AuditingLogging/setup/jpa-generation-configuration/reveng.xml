<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering 
  SYSTEM "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>
    <schema-selection match-schema="${qlack.db.schema}" />
    <type-mapping>
        <sql-type jdbc-type="CLOB" hibernate-type="String" />
        <sql-type jdbc-type="BLOB" hibernate-type="byte[]" />
        <sql-type jdbc-type="DECIMAL" length="255" precision="3" scale="0" hibernate-type="byte"/>
        <sql-type jdbc-type="DECIMAL" length="255" precision="1" scale="0" not-null="false" hibernate-type="java.lang.Boolean"/>
        <sql-type jdbc-type="DECIMAL" length="255" precision="1" scale="0" not-null="true" hibernate-type="boolean"/>
        <sql-type jdbc-type="DECIMAL" length="255" not-null="false" hibernate-type="java.lang.Long" />
        <sql-type jdbc-type="DECIMAL" length="255" not-null="true" hibernate-type="long" />
    </type-mapping>
	
    <table-filter match-name="al_.*" exclude="false"/>
    <table-filter match-name="AL_.*" exclude="false"/>
    <table name="al_audit">
        <!-- control many-to-one and set names for a specific named foreign key constraint -->
        <foreign-key constraint-name="fk_al_audit_traceId">
            <many-to-one property="customer"/>
        </foreign-key>
    </table>

</hibernate-reverse-engineering>